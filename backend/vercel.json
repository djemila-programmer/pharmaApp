{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.ts"
    }
  ],
  "env": {
    "DB_HOST": "votre-base-de-donnees-host",
    "DB_PORT": "3306",
    "DB_USERNAME": "votre-username",
    "DB_PASSWORD": "votre-password",
    "DB_NAME": "pharmacy_db",
    "JWT_SECRET": "votre-secret-jwt",
    "NODE_ENV": "production"
  }
}